{
	"$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
	"dest": "../../dist/core",
	"lib": {
		"entryFile": "src/public-api.ts",
		"styleIncludePaths": [
			"src/lib/styles",
			"src",
			"../../node_modules"
		]
	},
	"assets": [
		{
			"glob": "**/*",
			"input": "src/lib/assets",
			"output": "assets"
		}
	]
}
