{
	"$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
	"dest": "dist",
	"lib": {
		"entryFile": "src/public-api.ts",
		"styleIncludePaths": [
			"src",
			"../../node_modules"
		]
	},
	"inlineStyleLanguage": "scss",
	"assets": [
		{
			"glob": "**/*",
			"input": "assets",
			"output": "assets"
		}
	]
}
