{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"frakton-ng": {
			"projectType": "library",
			"root": "libs/frakton-ng",
			"sourceRoot": "libs/frakton-ng/src",
			"prefix": "fkt",
			"architect": {
				"build": {
					"builder": "@angular/build:ng-packagr",
					"configurations": {
						"production": {
							"tsConfig": "libs/frakton-ng/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "libs/frakton-ng/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular/build:karma",
					"options": {
						"tsConfig": "libs/frakton-ng/tsconfig.spec.json"
					}
				},
				"storybook": {
					"builder": "@storybook/angular:start-storybook",
					"options": {
						"configDir": "libs/frakton-ng/.storybook",
						"browserTarget": "frakton-ng:build:development",
						"compodoc": false,
						"port": 6006
					}
				},
				"build-storybook": {
					"builder": "@storybook/angular:build-storybook",
					"options": {
						"configDir": "libs/frakton-ng/.storybook",
						"browserTarget": "frakton-ng:build:production",
						"compodoc": false,
						"outputDir": "docs"
					}
				}
			}
		}
	},
	"schematics": {
		"@schematics/angular:component": {
			"type": "component",
			"style": "scss",
			"inlineStyle": false,
			"skipTests": true
		},
		"@schematics/angular:directive": {
			"type": "directive",
			"skipTests": true
		},
		"@schematics/angular:service": {
			"type": "service",
			"skipTests": true
		},
		"@schematics/angular:guard": {
			"typeSeparator": ".",
			"skipTests": true
		},
		"@schematics/angular:interceptor": {
			"typeSeparator": ".",
			"skipTests": true
		},
		"@schematics/angular:module": {
			"typeSeparator": ".",
			"skipTests": true
		},
		"@schematics/angular:pipe": {
			"typeSeparator": ".",
			"skipTests": true
		},
		"@schematics/angular:resolver": {
			"typeSeparator": ".",
			"skipTests": true
		}
	}
}
